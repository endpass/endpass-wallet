{
  "name": "endpass-wallet",
  "version": "1.0.0",
  "description": "Lightweight, easy to use Ethereum wallet",
  "author": "Endpass, Inc",
  "homepage": "https://wallet.endpass.com",
  "license": "MIT",
  "scripts": {
    "dev": "LOCAL_MODE=true vue-cli-service serve",
    "build": "vue-cli-service build",
    "dev-build": "vue-cli-service build --mode staging",
    "start": "yarn dev",
    "unit": "vue-cli-service test:unit",
    "unit:watch": "yarn unit --watch",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "test": "yarn unit",
    "pre-commit": "lint-staged",
    "lint": "eslint '**/*.{js,vue,json}' --quiet",
    "format:prettier:vue": "prettier --write '**/*.vue'",
    "format:eslint": "eslint --ext .json,.js,.vue src/ --fix --quiet",
    "format:prettier": "prettier --write '**/*.{js,json}'",
    "format": "yarn format:prettier:vue && yarn format:eslint && yarn format:prettier"
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix --quiet",
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "@endpass/class": "^0.10.5",
    "@endpass/connect": "0.17.5-beta",
    "@endpass/faucet": "^0.1.2",
    "@endpass/ui": "^0.4.1",
    "@endpass/utils": "^1.3.4",
    "ajv": "^6.5.5",
    "axios": "^0.18.0",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "bignumber.js": "^7.2.1",
    "bip39": "^2.5.0",
    "ethereum-blockies-base64": "^1.0.2",
    "ethereum-tx-decoder": "^2.0.1",
    "ethereumjs-wallet": "^0.6.0",
    "lodash": "^4.17.11",
    "secp256k1": "^3.5.2",
    "throttled-queue": "^1.0.5",
    "vee-validate": "^2.0.9",
    "vue": "2.5.22",
    "vue-analytics": "^5.12.2",
    "vue-intercom": "^1.0.12",
    "vue-multiselect": "^2.1.0",
    "vue-notification": "^1.3.7",
    "vue-router": "^3.0.1",
    "vue-router-multiguard": "^1.0.3",
    "vue-timers": "^1.7.3",
    "vuex": "^3.0.1",
    "web3": "1.0.0-beta.33",
    "web3-utils": "1.0.0-beta.33"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-jest": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^7.1.2",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.5",
    "babel-jest": "21.0.2",
    "babel-loader": "7.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "cypress": "^3.1.5",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^4.5.0",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.8.3",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.3",
    "sass-loader": "^6.0.7",
    "semver": "^5.3.0",
    "svg-inline-loader": "^0.8.0",
    "vue-template-compiler": "2.5.22",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie > 0"
  ]
}
